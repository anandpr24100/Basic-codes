// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n, x=2,i;
    //entering value from user
    cout<<"Enter the value of n: ";
    cin>>n;
    //running loop until we find out that much prime numbers
    while(n>0){
        //for checking if the number is prime or not from 2 till the number itself
        for(i =2;i<x;i++){
            //if number is not prime
            if(x%i ==0)
                break;
        }
        //if the number is prime
        if(i==x){
            cout<<x<<" ";
            n--;
        }
        x++;
         
         
    }

    return 0;
}
